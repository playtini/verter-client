{
    "name": "verter/client",
    "description": "Verter Client",
    "type": "library",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">= 7.4.6",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.2",
        "psr/log": "^1.1|^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "VerterClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VerterClient\\Tests\\": "tests/"
        }
    }
}
